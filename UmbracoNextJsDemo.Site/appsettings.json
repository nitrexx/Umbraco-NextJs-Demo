{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "uSync": {
    "Settings": {
      "ImportOnFirstBoot": true,
      "FirstBootGroup": "All",
      "ExportOnSave": "all",
      "DisableDashboard": false,
      "SummaryDashboard": true
    },
    "Sets": {
      "Default": {
        "HandlerDefaults": {
          "GuidNames": true
        }
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "5cf68a45-1511-423f-9005-274572b219a1",
        "SanitizeTinyMce": true
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "DeliveryApi": {
        "Enabled": true,
        "PublicAccess": true,
        "ApiKey": "",
        "DisallowedContentTypeAliases": [
          "topSecretDocumentType"
          /* this content type is not returned by the API*/
        ],
        "RichTextOutputAsJson": false,
        "Media": {
          "Enabled": true,
          "PublicAccess": true
        }
      },
      "Unattended": {
        "InstallUnattended": true,
        "UnattendedUserName": "Administrator",
        "UnattendedUserEmail": "demo@example.com",
        "UnattendedUserPassword": "Test123456@"
      },
      "Security": {
        "AllowConcurrentLogins": false
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=|DataDirectory|/Umbraco.sqlite.db;Cache=Shared;Foreign Keys=True;Pooling=True",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.Sqlite"
  }
}